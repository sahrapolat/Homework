print "Find the thief Program"

thief_dna =  ""
female_thief = "TGAAGGACCTTC"
blue_eyes = "TTGTGGTGGC"

if thief_dna.find(female_thief) is not -1:
    print("This is female!")

else:
    print ("Thief is male! Its either Matej or Miha...")

if thief_dna.find(blue_eyes) is not -1:
    print ("Thief has blue eyes! Its Matej!")


else:
    print ("Thief has green eyes. It can only be Miha.")
